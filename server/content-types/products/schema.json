{
  "kind": "collectionType",
  "collectionName": "image_marking_products",
  "singularName": "image-marking-product",
  "pluralName": "image-marking-products",
  "displayName": "Image Marking [Product]",
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "product_name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "product_desc": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "image_marking_images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::image-marking-image.image-marking-image"
    },
    "product_image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
